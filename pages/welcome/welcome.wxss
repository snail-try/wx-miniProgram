/* pages/welcome/welcome.wxss */
page{
  background-color:#b3d4db;
}
.container{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.headImage{
  height:300rpx;
  width:300rpx;
  border-radius:50%;
  margin-top:200rpx;
}

.welcomeText{
  font-size:32px;
  font-weight: bold;
  margin-top:100rpx;
}

.button{
  height:100rpx;
  width:350rpx;
  line-height:100rpx;
  border:solid 1rpx #405f80;
  border-radius:5px;
  text-align: center;
  margin-top:100rpx;
}

.buttonText{
  font-size:14px;
  color:#405f80;
  
}

